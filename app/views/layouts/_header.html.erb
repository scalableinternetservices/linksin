<script>
  $(document).ready(function(){
    const LI_Logo = document.getElementById("main-icon");
    const TLScale = new TimelineMax({ paused: true });
    TLScale.to(LI_Logo, 0.3, {transformOrigin: "50% 50%", scale: 1.2});
    LI_Logo.onmouseover = function() { TLScale.play() };
    LI_Logo.onmouseout = function() { TLScale.reverse() };
  });

  $("#nav-toggle").click(function() {
    $(".navbar-menu").toggleClass('is-active');
  });

</script>


